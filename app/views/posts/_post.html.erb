<div id="<%= dom_id post %>" class="post-container">
  <p>
    <%# <strong>Title:</strong> %>
    <%#= post.title %>
  </p>

  <p>
    <%# <strong>Description:</strong> %>
    <%#= post.description %>
  </p>

  <p>
    <%# <strong>Keywords:</strong> %>
    <%#= post.keywords %>
  </p>

  <p>
    <%#= post.user.email %>
  </p>

  <p>
    <%#= link_to "", post, class:"post-link", style:"z-index;" %>
  </p>
  <% if user_signed_in? %>
    <% like = current_user.likes.find_by(post: post) %>
    <% if like.nil? %>
      <%= button_to "Like", likes_path, params: { like: { post_id: post.id } }, method: :post %>
    <% else %>
      <%= button_to "Unlike", like_path(like), method: :delete %>
    <% end %>
  <% end %>
  <p> <%= post.likes.count %> Likes </p>

  <% post.images.each do |image| %>
    <%= image_tag(image, style: "object-fit: cover; width: 200px; height: 600px;") %>
  <% end %>
  <p>
    <%= post.title %>
  </p>

</div>

  <p>
    <%= link_to post.user.email, post.user, style:"z-index;" %>
  </p>
